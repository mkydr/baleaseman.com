u0628\u0631\u0627\u06cc \u0632\u0646\u062f\u06af\u06cc\u060c \u06a9\u0627\u0631 \u0648 \u062a\u062d\u0635\u06cc\u0644 \u062e\u0627\u0631\u062c\u06cc\u200c\u0647\u0627 \u0634\u062f\u0647 \u0627\u0633\u062a. \u062f\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0641\u0631\u0635\u062a\u200c\u0647\u0627\u06cc \u0634\u063a\u0644\u06cc \u0645\u0646\u0627\u0633\u0628\u06cc \u0628\u0631\u0627\u06cc \u0645\u062a\u062e\u0635\u0635\u0627\u0646 \u062f\u0631 \u0632\u0645\u06cc\u0646\u0647\u200c\u0647\u0627\u06cc \u0645\u062e\u062a\u0644\u0641 \u0648\u062c\u0648\u062f \u062f\u0627\u0631\u062f \u0648 \u062f\u0631 \u0635\u0648\u0631\u062a \u0628\u0631\u062e\u0648\u0631\u062f\u0627\u0631\u06cc \u0627\u0632 \u06cc\u06a9 \u0645\u0647\u0627\u0631\u062a \u062e\u0627\u0635 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u06cc\u062f \u062f\u0631 \u0627\u06cc\u0646 \u06a9\u0634\u0648\u0631 \u067e\u0647\u0646\u0627\u0648\u0631 \u0645\u0634\u063a\u0648\u0644 \u0628\u0647 \u06a9\u0627\u0631 \u0634\u0648\u06cc\u062f. \u0627\u0645\u0627 \u06cc\u0627\u0641\u062a\u0646 \u0634\u063a\u0644 \u062f\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0686\u06af\u0648\u0646\u0647 \u0627\u0633\u062a\u061f<\/p>\n<p><strong>\u0646\u062d\u0648\u0647 \u067e\u06cc\u062f\u0627 \u06a9\u0631\u062f\u0646 \u06a9\u0627\u0631 \u062f\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627<\/strong><\/p>\n<p>\u0628\u0631\u0627\u06cc \u06cc\u0627\u0641\u062a\u0646 \u0634\u063a\u0644 \u062f\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0628\u0627\u06cc\u062f \u06cc\u06a9 \u0628\u0631\u0646\u0627\u0645\u0647\u200c\u0631\u06cc\u0632\u06cc \u0627\u0632 \u067e\u06cc\u0634 \u062a\u0639\u06cc\u06cc\u0646 \u0634\u062f\u0647 \u0631\u0627 \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u06cc\u062f. \u0648\u0631\u0648\u062f \u0628\u0647 \u0627\u06cc\u0646 \u06a9\u0634\u0648\u0631 \u0628\u062f\u0648\u0646 \u0627\u06cc\u0646 \u06a9\u0647 \u0627\u0632 \u0642\u0628\u0644 \u0622\u0645\u0627\u062f\u0647\u200c\u0633\u0627\u0632\u06cc \u0645\u0642\u062f\u0645\u0627\u062a\u06cc \u0631\u0627 \u0628\u0631\u0627\u06cc \u06cc\u0627\u0641\u062a\u0646 \u0634\u063a\u0644 \u062f\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0627\u0646\u062c\u0627\u0645 \u062f\u0627\u062f\u0647 \u0628\u0627\u0634\u06cc\u062f \u0634\u0645\u0627 \u0631\u0627 \u062f\u0686\u0627\u0631 \u0645\u0634\u06a9\u0644\u0627\u062a\u06cc \u062e\u0648\u0627\u0647\u062f \u06a9\u0631\u062f. \u062d\u062a\u06cc \u0627\u06af\u0631 \u062f\u0631 \u06cc\u06a9 \u0632\u0645\u06cc\u0646\u0647\u060c \u0645\u0647\u0627\u0631\u062a \u0628\u0633\u06cc\u0627\u0631 \u062e\u0648\u0628\u06cc \u0647\u0645 \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u06cc\u062f \u0645\u0645\u06a9\u0646 \u0627\u0633\u062a \u0634\u0634 \u0645\u0627\u0647 \u0632\u0645\u0627\u0646 \u0628\u0628\u0631\u062f \u062a\u0627 \u0628\u062a\u0648\u0627\u0646\u06cc\u062f \u06cc\u06a9 \u0634\u063a\u0644 \u0645\u0646\u0627\u0633\u0628 \u0631\u0627 \u062f\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u067e\u06cc\u062f\u0627 \u06a9\u0646\u06cc\u062f. \u0628\u0646\u0627\u0628\u0631\u0627\u06cc\u0646 \u0627\u06af\u0631 \u0628\u062f\u0648\u0646 \u0628\u0631\u0646\u0627\u0645\u0647 \u0648\u0627\u0631\u062f \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0634\u0648\u06cc\u062f \u0645\u062c\u0628\u0648\u0631 \u062e\u0648\u0627\u0647\u06cc\u062f \u0634\u062f \u06a9\u0647 \u0686\u0646\u062f\u06cc\u0646 \u0645\u0627\u0647 \u062f\u0631 \u0645\u0634\u0627\u063a\u0644 \u0633\u062e\u062a \u0648 \u0633\u0637\u062d \u067e\u0627\u06cc\u06cc\u0646 \u0641\u0639\u0627\u0644\u06cc\u062a \u06a9\u0646\u06cc\u062f \u062a\u0627 \u0628\u062a\u0648\u0627\u0646\u06cc\u062f \u0627\u0632 \u067e\u0633 \u0647\u0632\u06cc\u0646\u0647\u200c\u0647\u0627\u06cc \u062e\u0648\u062f \u0628\u0631\u0628\u06cc\u0627\u06cc\u06cc\u062f. \u0646\u062d\u0648\u0647 \u067e\u06cc\u062f\u0627 \u06a9\u0631\u062f\u0646 \u06a9\u0627\u0631 \u062f\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0645\u0631\u0627\u062d\u0644\u06cc \u062f\u0627\u0631\u062f \u06a9\u0647 \u062f\u0631 \u0627\u062f\u0627\u0645\u0647 \u0628\u0631\u0631\u0633\u06cc \u062e\u0648\u0627\u0647\u062f \u0634\u062f.<\/p>\n<p><strong>\u0645\u0631\u062d\u0644\u0647 \u0627\u0648\u0644: \u062c\u0633\u062a\u062c\u0648\u06cc \u0634\u063a\u0644 \u062f\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627<\/strong><\/p>\n<p>\u0627\u0648\u0644\u06cc\u0646 \u06a9\u0627\u0631\u06cc \u06a9\u0647 \u0628\u0631\u0627\u06cc \u06cc\u0627\u0641\u062a\u0646 \u0634\u063a\u0644 \u062f\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0628\u0627\u06cc\u062f \u0627\u0646\u062c\u0627\u0645 \u062f\u0647\u06cc\u062f \u062c\u0633\u062a\u062c\u0648 \u0628\u0631\u0627\u06cc \u067e\u06cc\u062f\u0627 \u06a9\u0631\u062f\u0646 \u06cc\u06a9 \u06a9\u0627\u0631 \u0645\u0646\u0627\u0633\u0628 \u062f\u0631 \u062d\u0648\u0632\u0647 \u062a\u062e\u0635\u0635\u06cc \u062e\u0648\u062f\u062a\u0627\u0646 \u0627\u0633\u062a. \u062f\u0642\u062a \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u06cc\u062f \u06a9\u0647 \u0628\u0627\u06cc\u062f \u062f\u0631 \u0634\u063a\u0644 \u0645\u0648\u0631\u062f \u0646\u0638\u0631 \u062f\u0627\u0631\u0627\u06cc \u0633\u0627\u0628\u0642\u0647 \u06a9\u0627\u0631\u06cc \u0648 \u0645\u0647\u0627\u0631\u062a \u06a9\u0627\u0641\u06cc \u0628\u0627\u0634\u06cc\u062f. \u0628\u0647\u062a\u0631\u06cc\u0646 \u0631\u0648\u0634 \u0628\u0631\u0627\u06cc \u06cc\u0627\u0641\u062a\u0646 \u0634\u063a\u0644 \u062f\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0627\u06cc\u0646 \u0627\u0633\u062a \u06a9\u0647 \u062f\u0631 \u0633\u0627\u06cc\u062a\u200c\u0647\u0627\u06cc \u06a9\u0627\u0631\u06cc\u0627\u0628\u06cc \u0628\u06cc\u0646\u200c\u0627\u0644\u0645\u0644\u0644\u06cc \u0628\u0647 \u062c\u0633\u062a\u062c\u0648\u06cc \u0641\u0631\u0635\u062a\u200c\u0647\u0627\u06cc \u0634\u063a\u0644\u06cc \u0645\u0648\u062c\u0648\u062f \u062f\u0631 \u062d\u0648\u0632\u0647 \u062a\u062e\u0635\u0635\u06cc \u062e\u0648\u062f\u062a\u0627\u0646 \u0628\u067e\u0631\u062f\u0627\u0632\u06cc\u062f. \u062f\u0631 \u0627\u062f\u0627\u0645\u0647 \u062a\u0639\u062f\u0627\u062f\u06cc \u0627\u0632 \u0628\u0647\u062a\u0631\u06cc\u0646 \u0633\u0627\u06cc\u062a\u200c\u0647\u0627\u06cc \u06a9\u0627\u0631\u06cc\u0627\u0628\u06cc \u0645\u0639\u0631\u0641\u06cc \u0634\u062f\u0647\u200c\u0627\u0646\u062f:<\/p>\n<ul>\n<li><a href=\"https:\/\/www.adzuna.com.au\/\">Adzuna<\/a><\/li>\n<li><a href=\"https:\/\/jobsearch.gov.au\/\">Job active &#8211; Australia<\/a><\/li>\n<li><a href=\"https:\/\/www.careerone.com.au\/\">Careerone<\/a><\/li>\n<li><a href=\"https:\/\/gradaustralia.com.au\/\">GradAustralia<\/a><\/li>\n<li><a href=\"https:\/\/au.gradconnection.com\/\">GradConnection<\/a><\/li>\n<li><a href=\"https:\/\/www.seek.com.au\/\">Seek<\/a><\/li>\n<\/ul>\n<p>\u0646\u06a9\u062a\u0647 \u0645\u0647\u0645 \u062f\u0631 \u0627\u06cc\u0646 \u0631\u0627\u0628\u0637\u0647 \u0627\u06cc\u0646 \u0627\u0633\u062a \u06a9\u0647 \u0634\u063a\u0644 \u0648 \u062a\u062e\u0635\u0635 \u0645\u0648\u0631\u062f \u0646\u0638\u0631 \u0634\u0645\u0627 \u062d\u062a\u0645\u0627 \u062f\u0631\u00a0<a href=\"https:\/\/www.seekvisa.com.au\/skilled-occupation-list-australia-2019\/\">\u0644\u06cc\u0633\u062a \u0645\u0634\u0627\u063a\u0644 \u0645\u0648\u0631\u062f \u0646\u06cc\u0627\u0632 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627<\/a>\u00a0\u0622\u0645\u062f\u0647 \u0628\u0627\u0634\u062f. \u062f\u0631 \u062d\u0627\u0644 \u062d\u0627\u0636\u0631 \u06a9\u0634\u0648\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u062f\u0631 \u0628\u0631\u062e\u06cc \u062d\u0648\u0632\u0647\u200c\u0647\u0627\u06cc \u06a9\u0627\u0631\u06cc \u062f\u0627\u0631\u0627\u06cc \u06a9\u0645\u0628\u0648\u062f \u0646\u06cc\u0631\u0648\u06cc \u06a9\u0627\u0631 \u0645\u0627\u0647\u0631 \u0627\u0633\u062a \u0627\u0632 \u062c\u0645\u0644\u0647 \u0627\u06cc\u0646 \u062d\u0648\u0632\u0647\u200c\u0647\u0627 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646 \u0628\u0647 \u0645\u0648\u0627\u0631\u062f \u0632\u06cc\u0631 \u0627\u0634\u0627\u0631\u0647 \u062f\u0627\u0634\u062a:<\/p>\n<ul>\n<li>\u0627\u0645\u0648\u0631 \u0645\u0627\u0644\u06cc<\/li>\n<li>\u0633\u0627\u062e\u062a \u0648 \u0633\u0627\u0632<\/li>\n<li>\u0622\u0645\u0648\u0632\u0634<\/li>\n<li>\u0645\u0647\u0646\u062f\u0633\u06cc<\/li>\n<li>\u0628\u0647\u062f\u0627\u0634\u062a \u0648 \u062f\u0631\u0645\u0627\u0646<\/li>\n<\/ul>\n<p>\u0645\u0648\u0636\u0648\u0639 \u062f\u06cc\u06af\u0631\u06cc \u06a9\u0647 \u062f\u0631 \u0627\u06cc\u0646 \u0645\u0648\u0631\u062f \u0648\u062c\u0648\u062f \u062f\u0627\u0631\u062f \u0627\u0646\u062a\u062e\u0627\u0628 \u0627\u06cc\u0627\u0644\u062a \u0648 \u0634\u0647\u0631 \u0645\u0648\u0631\u062f \u0646\u0638\u0631 \u0628\u0631\u0627\u06cc \u06a9\u0627\u0631 \u06a9\u0631\u062f\u0646 \u0627\u0633\u062a. \u0634\u0647\u0631\u0647\u0627\u06cc \u0645\u062e\u062a\u0644\u0641 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0628\u0647 \u0644\u062d\u0627\u0638 \u0634\u0631\u0627\u06cc\u0637 \u0632\u0646\u062f\u06af\u06cc \u0648 \u0647\u0645\u0686\u0646\u06cc\u0646 \u0645\u0634\u0627\u063a\u0644 \u0645\u0648\u0631\u062f \u0646\u06cc\u0627\u0632 \u0628\u0627 \u06cc\u06a9\u062f\u06cc\u06af\u0631 \u062a\u0641\u0627\u0648\u062a\u200c\u0647\u0627\u06cc\u06cc \u0631\u0627 \u062f\u0627\u0631\u0646\u062f. \u0628\u0647 \u0639\u0646\u0648\u0627\u0646 \u0645\u062b\u0627\u0644 \u0627\u06af\u0631 \u062f\u0631 \u0632\u0645\u06cc\u0646\u0647\u200c\u0647\u0627\u06cc\u06cc \u0646\u0638\u06cc\u0631 \u0627\u0645\u0648\u0631 \u0645\u0627\u0644\u06cc\u060c IT\u060c \u0639\u0644\u0648\u0645 \u0632\u06cc\u0633\u062a\u06cc \u0648 \u062a\u0648\u0644\u06cc\u062f \u0627\u0646\u0631\u0698\u06cc \u062f\u0627\u0631\u0627\u06cc \u062a\u062e\u0635\u0635 \u0648 \u0645\u0647\u0627\u0631\u062a \u0628\u0627\u0634\u06cc\u062f\u00a0<a href=\"https:\/\/nilgamsafar.com\/blog\/work-in-brisbane-australia\">\u06a9\u0627\u0631 \u062f\u0631 \u0628\u0631\u06cc\u0632\u0628\u06cc\u0646 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627<\/a>\u00a0\u06af\u0632\u06cc\u0646\u0647 \u0645\u0637\u0644\u0648\u0628\u06cc \u0628\u0631\u0627\u06cc \u0634\u0645\u0627 \u062e\u0648\u0627\u0647\u062f \u0628\u0648\u062f.<\/p>\n<p>\u0628\u0639\u062f \u0627\u0632 \u0627\u06cc\u0646 \u06a9\u0647 \u0634\u063a\u0644 \u0645\u062a\u0646\u0627\u0633\u0628 \u0628\u0627 \u0645\u0647\u0627\u0631\u062a\u200c\u0647\u0627\u06cc \u062e\u0648\u062f \u0631\u0627 \u062f\u0631 \u0628\u06cc\u0646 \u0645\u0634\u0627\u063a\u0644 \u0645\u0648\u0631\u062f \u0646\u06cc\u0627\u0632 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u067e\u06cc\u062f\u0627 \u06a9\u0631\u062f\u06cc\u062f \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u06cc\u062f \u0627\u0642\u062f\u0627\u0645\u0627\u062a \u0628\u0639\u062f\u06cc \u0631\u0627 \u062f\u0631 \u062f\u0633\u062a\u0648\u0631 \u06a9\u0627\u0631 \u062e\u0648\u062f \u0642\u0631\u0627\u0631 \u062f\u0647\u06cc\u062f. \u06cc\u06a9\u06cc \u0627\u0632 \u0627\u06cc\u0646 \u0627\u0642\u062f\u0627\u0645\u0627\u062a \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u062f \u0627\u0631\u0633\u0627\u0644 \u0631\u0632\u0648\u0645\u0647 \u0628\u0647 \u06a9\u0627\u0631\u0641\u0631\u0645\u0627\u06cc\u0627\u0646 \u06a9\u0634\u0648\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0628\u0627\u0634\u062f.<\/p>\n<p><img decoding=\"async\" loading=\"lazy\" class=\"aligncenter wp-image-15457 size-full\" src=\"https:\/\/nilgamsafar.com\/wp-content\/uploads\/2023\/06\/tech-jobs-1.jpg\" alt=\"\" width=\"600\" height=\"400\" srcset=\"https:\/\/nilgamsafar.com\/wp-content\/uploads\/2023\/06\/tech-jobs-1.jpg 600w, https:\/\/nilgamsafar.com\/wp-content\/uploads\/2023\/06\/tech-jobs-1-300x200.jpg 300w, https:\/\/nilgamsafar.com\/wp-content\/uploads\/2023\/06\/tech-jobs-1-148x99.jpg 148w\" sizes=\"(max-width: 600px) 100vw, 600px\" \/><\/p>\n<p><strong>\u0645\u0631\u062d\u0644\u0647 \u062f\u0648\u0645: \u06af\u0631\u0641\u062a\u0646 \u0648\u06cc\u0632\u0627\u06cc \u06a9\u0627\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627<\/strong><\/p>\n<p>\u06f1- \u062a\u0639\u06cc\u06cc\u0646 \u0646\u0648\u0639 \u0648\u06cc\u0632\u0627\u06cc \u06a9\u0627\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627<\/p>\n<p>\u062f\u0631 \u062d\u0627\u0644 \u062d\u0627\u0636\u0631 \u0627\u0646\u0648\u0627\u0639 \u0645\u062e\u062a\u0644\u0641\u06cc \u0627\u0632 \u0648\u06cc\u0632\u0627\u06cc \u06a9\u0627\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0648\u062c\u0648\u062f \u062f\u0627\u0631\u062f \u06a9\u0647 \u0628\u0631\u0627\u06cc \u0627\u0637\u0644\u0627\u0639 \u0627\u0632 \u062c\u0632\u0626\u06cc\u0627\u062a \u0647\u0631 \u06cc\u06a9 \u0627\u0632 \u0622\u0646\u200c\u0647\u0627 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u06cc\u062f \u0628\u0647\u00a0<a href=\"https:\/\/immi.homeaffairs.gov.au\/visas\/getting-a-visa\/visa-listing\">\u0633\u0627\u06cc\u062a \u0627\u062f\u0627\u0631\u0647 \u0645\u0647\u0627\u062c\u0631\u062a \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627<\/a>\u00a0\u0645\u0631\u0627\u062c\u0639\u0647 \u0646\u0645\u0627\u06cc\u06cc\u062f. \u0627\u0645\u0627 \u062f\u0631 \u0627\u06cc\u0646\u062c\u0627 \u0628\u0647 \u062f\u0648 \u0646\u0648\u0639 \u0631\u0627\u06cc\u062c \u0648\u06cc\u0632\u0627\u06cc \u06a9\u0627\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u062e\u0648\u0627\u0647\u06cc\u0645 \u067e\u0631\u062f\u0627\u062e\u062a.<\/p>\n<ul>\n<li>\u0648\u06cc\u0632\u0627\u06cc \u0645\u0647\u0627\u0631\u062a \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0628\u0647 \u0635\u0648\u0631\u062a \u0645\u0633\u062a\u0642\u0644<\/li>\n<\/ul>\n<p>\u0627\u06cc\u0646 \u0648\u06cc\u0632\u0627 \u0645\u062e\u0635\u0648\u0635 \u0645\u0647\u0627\u062c\u0631\u0627\u0646\u06cc \u0627\u0633\u062a \u06a9\u0647 \u062f\u0631 \u06cc\u06a9 \u0632\u0645\u06cc\u0646\u0647 \u062a\u062e\u0635\u0635\u06cc \u0645\u0648\u0631\u062f \u0646\u06cc\u0627\u0632 \u0628\u0627\u0632\u0627\u0631 \u06a9\u0627\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u062f\u0627\u0631\u0627\u06cc \u0645\u0647\u0627\u0631\u062a \u0628\u0627\u0634\u0646\u062f. \u0628\u0631\u0627\u06cc \u0627\u062e\u0630 \u0648\u06cc\u0632\u0627\u06cc \u0645\u0647\u0627\u0631\u062a \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0628\u0647 \u0635\u0648\u0631\u062a \u0645\u0633\u062a\u0642\u0644 \u0628\u0627\u06cc\u062f \u0648\u0627\u062c\u062f \u0634\u0631\u0627\u06cc\u0637\u06cc \u0628\u0627\u0634\u06cc\u062f. \u0627\u0632 \u062c\u0645\u0644\u0647 \u0627\u06cc\u0646 \u0634\u0631\u0627\u06cc\u0637 \u0628\u0627\u06cc\u062f \u0628\u0647 \u0633\u0646\u060c \u0632\u0628\u0627\u0646 \u0627\u0646\u06af\u0644\u06cc\u0633\u06cc \u0648 \u0627\u062b\u0628\u0627\u062a \u0645\u0647\u0627\u0631\u062a \u0634\u063a\u0644\u06cc \u0627\u0634\u0627\u0631\u0647 \u062f\u0627\u0634\u062a.<\/p>\n<ul>\n<li>\u0648\u06cc\u0632\u0627\u06cc \u062d\u0645\u0627\u06cc\u062a\u06cc \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627<\/li>\n<\/ul>\n<p>\u0627\u0641\u0631\u0627\u062f\u06cc \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u0646\u062f \u0628\u0631\u0627\u06cc \u0627\u062e\u0630 \u0648\u06cc\u0632\u0627\u06cc \u062d\u0645\u0627\u06cc\u062a\u06cc \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0627\u0642\u062f\u0627\u0645 \u0646\u0645\u0627\u06cc\u0646\u062f \u06a9\u0647 \u062f\u0631 \u06cc\u06a9 \u0632\u0645\u06cc\u0646\u0647 \u062a\u062e\u0635\u0635\u06cc \u062f\u0627\u0631\u0627\u06cc \u0645\u0647\u0627\u0631\u062a \u0628\u0648\u062f\u0647 \u0648 \u062a\u0648\u0633\u0637 \u06cc\u06a9 \u06a9\u0627\u0631\u0641\u0631\u0645\u0627\u06cc \u0645\u0648\u0631\u062f \u062a\u0627\u06cc\u06cc\u062f \u062f\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u062d\u0645\u0627\u06cc\u062a \u0634\u062f\u0647 \u0628\u0627\u0634\u0646\u062f. \u0628\u0631\u0627\u06cc \u0627\u062e\u0630 \u0648\u06cc\u0632\u0627\u06cc \u062d\u0645\u0627\u06cc\u062a\u06cc \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0646\u06cc\u0632 \u0628\u0627\u06cc\u062f \u0627\u0644\u0632\u0627\u0645\u0627\u062a\u06cc \u0631\u0627 \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u06cc\u062f \u0627\u0645\u0627 \u062f\u0631 \u0631\u0648\u0646\u062f \u0627\u062e\u0630 \u0627\u06cc\u0646 \u0648\u06cc\u0632\u0627 \u06a9\u0627\u0631\u0641\u0631\u0645\u0627\u06cc \u0634\u0645\u0627 \u062a\u0627\u062b\u06cc\u0631 \u0632\u06cc\u0627\u062f\u06cc \u0631\u0627 \u062e\u0648\u0627\u0647\u062f \u062f\u0627\u0634\u062a.<\/p>\n<p>\u06f2- \u062f\u0631\u062e\u0648\u0627\u0633\u062a \u0648\u06cc\u0632\u0627\u06cc \u06a9\u0627\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627<\/p>\n<p>\u062f\u0631 \u062d\u0627\u0644 \u062d\u0627\u0636\u0631\u060c \u0627\u062f\u0627\u0631\u0647 \u0645\u0647\u0627\u062c\u0631\u062a \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0627\u0645\u06a9\u0627\u0646\u06cc \u0631\u0627 \u0641\u0631\u0627\u0647\u0645 \u06a9\u0631\u062f\u0647 \u062a\u0627 \u0627\u0641\u0631\u0627\u062f \u062e\u0627\u0631\u062c\u06cc \u062f\u0627\u0631\u0627\u06cc \u0645\u0647\u0627\u0631\u062a \u0628\u062a\u0648\u0627\u0646\u0646\u062f \u06cc\u06a9 \u0641\u0631\u0645 \u0627\u0639\u0644\u0627\u0645 \u0622\u0645\u0627\u062f\u06af\u06cc \u062a\u062d\u062a \u0639\u0646\u0648\u0627\u0646 EOI \u0631\u0627 \u0627\u0631\u0633\u0627\u0644 \u0646\u0645\u0627\u06cc\u0646\u062f. \u0628\u0631\u0627\u06cc\u00a0<a href=\"https:\/\/skillselect.gov.au\/SKILLSELECTSTS\/Login.aspx?ReturnUrl=%2fSKILLSELECTSTS%2f%3fwa%3dwsignin1.0%26wtrealm%3dhttp%253a%252f%252fauth.dis.gov.au%252fadfs%252fservices%252ftrust%26wctx%3dfacce369-cc7b-453b-b2df-767409237440%26client-request-id%3daaa2da70-36e7-4fea-7217-008000040027&amp;wa=wsignin1.0&amp;wtrealm=http%3a%2f%2fauth.dis.gov.au%2fadfs%2fservices%2ftrust&amp;wctx=facce369-cc7b-453b-b2df-767409237440&amp;client-request-id=aaa2da70-36e7-4fea-7217-008000040027\">\u0627\u0631\u0633\u0627\u0644 \u0641\u0631\u0645 \u0627\u0639\u0644\u0627\u0645 \u0622\u0645\u0627\u062f\u06af\u06cc \u06a9\u0627\u0631 \u062f\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627<\/a>\u00a0\u0628\u0627\u06cc\u062f \u062f\u0631 \u0642\u0633\u0645\u062a SkillSelect \u0633\u0627\u06cc\u062a \u0627\u062f\u0627\u0631\u0647 \u0645\u0647\u0627\u062c\u0631\u062a \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u062b\u0628\u062a \u0646\u0627\u0645 \u0646\u0645\u0627\u06cc\u06cc\u062f.<\/p>\n<ul>\n<li>\u062f\u0631\u062e\u0648\u0627\u0633\u062a \u0648\u06cc\u0632\u0627\u06cc \u0645\u0647\u0627\u0631\u062a \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627<\/li>\n<\/ul>\n<p>\u0628\u0631\u0627\u06cc \u0627\u06cc\u0646 \u06a9\u0647 \u0628\u062a\u0648\u0627\u0646\u06cc\u062f \u0648\u06cc\u0632\u0627\u06cc \u0645\u0647\u0627\u0631\u062a \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0631\u0627 \u0628\u0647 \u0635\u0648\u0631\u062a \u0645\u0633\u062a\u0642\u0644 \u062f\u0631\u06cc\u0627\u0641\u062a \u06a9\u0646\u06cc\u062f \u0636\u0631\u0648\u0631\u06cc \u0627\u0633\u062a \u06a9\u0647 \u06cc\u06a9 \u067e\u0631\u0648\u0641\u0627\u06cc\u0644 \u0634\u062e\u0635\u06cc \u0631\u0627 \u062f\u0631 SkillSelect \u0627\u06cc\u062c\u0627\u062f \u06a9\u0631\u062f\u0647 \u0648 \u062a\u0648\u0627\u0646\u0645\u0646\u062f\u06cc\u200c\u0647\u0627\u06cc \u062e\u0648\u062f \u0631\u0627 \u0627\u0631\u0627\u0626\u0647 \u0646\u0645\u0627\u06cc\u06cc\u062f. \u062f\u0642\u062a \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u06cc\u062f \u06a9\u0647 \u062b\u0628\u062a EOI \u062f\u0631 \u0633\u0627\u06cc\u062a \u0645\u0630\u06a9\u0648\u0631 \u0628\u0647 \u0645\u0639\u0646\u0627\u06cc \u062f\u0631\u062e\u0648\u0627\u0633\u062a \u0648\u06cc\u0632\u0627 \u0646\u06cc\u0633\u062a \u0648 \u0647\u06cc\u0686 \u0647\u0632\u06cc\u0646\u0647\u200c\u0627\u06cc \u0631\u0627 \u0646\u062e\u0648\u0627\u0647\u062f \u062f\u0627\u0634\u062a. \u0628\u0639\u062f \u0627\u0632 \u0627\u0646\u062c\u0627\u0645 \u0627\u06cc\u0646 \u0645\u0631\u062d\u0644\u0647 \u0628\u0627\u06cc\u062f \u0645\u0646\u062a\u0638\u0631 \u0628\u0645\u0627\u0646\u06cc\u062f \u062a\u0627 \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0634\u0645\u0627 \u0645\u0648\u0631\u062f \u0627\u0631\u0632\u06cc\u0627\u0628\u06cc \u0642\u0631\u0627\u0631 \u0628\u06af\u06cc\u0631\u062f \u0648 \u062f\u0631 \u0635\u0648\u0631\u062a \u0648\u0627\u062c\u062f \u0634\u0631\u0627\u06cc\u0637 \u0628\u0648\u062f\u0646 \u062f\u0639\u0648\u062a\u0646\u0627\u0645\u0647\u200c\u0627\u06cc \u0628\u0631\u0627\u06cc \u0634\u0645\u0627 \u0627\u0631\u0633\u0627\u0644 \u062e\u0648\u0627\u0647\u062f \u0634\u062f. \u0628\u0639\u062f \u0627\u0632 \u062f\u0631\u06cc\u0627\u0641\u062a \u0627\u06cc\u0646 \u062f\u0639\u0648\u062a\u0646\u0627\u0645\u0647 \u0634\u0634 \u0645\u0627\u0647 \u0632\u0645\u0627\u0646 \u062e\u0648\u0627\u0647\u06cc\u062f \u062f\u0627\u0634\u062a \u062a\u0627 \u0628\u0631\u0627\u06cc \u0627\u062e\u0630 \u0648\u06cc\u0632\u0627 \u0627\u0642\u062f\u0627\u0645 \u06a9\u0646\u06cc\u062f.<\/p>\n<ul>\n<li>\u062f\u0631\u062e\u0648\u0627\u0633\u062a \u0648\u06cc\u0632\u0627\u06cc \u062d\u0645\u0627\u06cc\u062a\u06cc \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627<\/li>\n<\/ul>\n<p>\u062a\u0641\u0627\u0648\u062a \u0627\u0635\u0644\u06cc \u062f\u0631 \u0627\u062e\u0630 \u0648\u06cc\u0632\u0627\u06cc \u062d\u0645\u0627\u06cc\u062a\u06cc \u0648 \u0648\u06cc\u0632\u0627\u06cc \u0645\u0647\u0627\u0631\u062a \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u062f\u0631 \u0627\u06cc\u0646 \u0646\u06a9\u062a\u0647 \u0627\u0633\u062a \u06a9\u0647 \u0628\u0631\u0627\u06cc \u0627\u062e\u0630 \u0648\u06cc\u0632\u0627\u06cc \u062d\u0645\u0627\u06cc\u062a\u06cc \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0628\u0627\u06cc\u062f \u062f\u0631 \u0627\u0628\u062a\u062f\u0627 \u06cc\u06a9 \u0634\u063a\u0644 \u0631\u0627 \u067e\u06cc\u062f\u0627 \u06a9\u0646\u06cc\u062f. \u062f\u0631 \u0648\u0627\u0642\u0639 \u0628\u0627\u06cc\u062f \u0641\u0631\u0635\u062a\u200c\u0647\u0627\u06cc \u0634\u063a\u0644\u06cc \u062f\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0631\u0627 \u0627\u0632 \u0637\u0631\u06cc\u0642 \u0633\u0627\u06cc\u062a\u200c\u0647\u0627\u06cc \u06a9\u0627\u0631\u06cc\u0627\u0628\u06cc \u0645\u0639\u0631\u0648\u0641 \u062f\u0646\u0628\u0627\u0644 \u06a9\u0631\u062f\u0647 \u0648 \u062f\u0631\u062e\u0648\u0627\u0633\u062a \u062e\u0648\u062f \u0631\u0627 \u0628\u0631\u0627\u06cc \u06a9\u0627\u0631\u0641\u0631\u0645\u0627\u0647\u0627\u06cc \u0645\u062e\u062a\u0644\u0641 \u0628\u0641\u0631\u0633\u062a\u06cc\u062f. \u062f\u0642\u062a \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u06cc\u062f \u06a9\u0647 \u062f\u0631 \u0627\u06cc\u0646 \u0645\u0631\u062d\u0644\u0647\u060c \u0631\u0632\u0648\u0645\u0647 \u06a9\u0627\u0631\u06cc \u0634\u0645\u0627 \u0627\u0647\u0645\u06cc\u062a \u0628\u0633\u06cc\u0627\u0631 \u0632\u06cc\u0627\u062f\u06cc \u0631\u0627 \u062f\u0631 \u062a\u0635\u0645\u06cc\u0645\u200c\u06af\u06cc\u0631\u06cc \u06a9\u0627\u0631\u0641\u0631\u0645\u0627\u06cc \u0645\u0648\u0631\u062f \u0646\u0638\u0631 \u062e\u0648\u0627\u0647\u062f \u062f\u0627\u0634\u062a. \u062f\u0631 \u0635\u0648\u0631\u062a\u06cc \u06a9\u0647 \u0628\u062a\u0648\u0627\u0646\u06cc\u062f \u062d\u0645\u0627\u06cc\u062a \u06cc\u06a9 \u06a9\u0627\u0631\u0641\u0631\u0645\u0627\u06cc \u0645\u0648\u0631\u062f \u062a\u0627\u06cc\u06cc\u062f \u062f\u0631 \u06a9\u0634\u0648\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0631\u0627 \u062c\u0644\u0628 \u06a9\u0646\u06cc\u062f \u06a9\u0627\u0631\u0641\u0631\u0645\u0627\u06cc \u0645\u0630\u06a9\u0648\u0631 \u062f\u0631\u062e\u0648\u0627\u0633\u062a\u06cc \u0631\u0627 \u0645\u0628\u0646\u06cc \u0628\u0631 \u062d\u0645\u0627\u06cc\u062a \u0627\u0632 \u0634\u0645\u0627 \u0627\u0631\u0627\u0626\u0647 \u0645\u06cc\u200c\u06a9\u0646\u062f. \u062a\u0646\u0647\u0627 \u0632\u0645\u0627\u0646\u06cc \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u06cc\u062f \u0628\u0631\u0627\u06cc \u0648\u06cc\u0632\u0627\u06cc \u062d\u0645\u0627\u06cc\u062a\u06cc \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0627\u0642\u062f\u0627\u0645 \u06a9\u0646\u06cc\u062f \u06a9\u0647 \u0627\u06cc\u0646 \u0646\u0627\u0645\u0647 \u0631\u0627 \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u06cc\u062f.<\/p>\n<p><img decoding=\"async\" loading=\"lazy\" class=\"aligncenter wp-image-15456 size-full\" src=\"https:\/\/nilgamsafar.com\/wp-content\/uploads\/2023\/06\/employment-in-australia-1.jpg\" alt=\"\" width=\"600\" height=\"309\" srcset=\"https:\/\/nilgamsafar.com\/wp-content\/uploads\/2023\/06\/employment-in-australia-1.jpg 600w, https:\/\/nilgamsafar.com\/wp-content\/uploads\/2023\/06\/employment-in-australia-1-300x155.jpg 300w, https:\/\/nilgamsafar.com\/wp-content\/uploads\/2023\/06\/employment-in-australia-1-148x76.jpg 148w\" sizes=\"(max-width: 600px) 100vw, 600px\" \/><\/p>\n<p>\u06f3- \u0628\u0631\u0631\u0633\u06cc \u0648\u0627\u062c\u062f \u0634\u0631\u0627\u06cc\u0637 \u0628\u0648\u062f\u0646<\/p>\n<p>\u0627\u062f\u0627\u0631\u0647 \u0645\u0647\u0627\u062c\u0631\u062a \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0628\u0647 \u0645\u0646\u0638\u0648\u0631 \u0633\u0627\u0632\u0645\u0627\u0646\u200c\u062f\u0647\u06cc \u0627\u0645\u0648\u0631 \u0645\u0631\u0628\u0648\u0637 \u0628\u0647 \u0645\u0647\u0627\u062c\u0631\u0627\u0646 \u06cc\u06a9\u00a0<a href=\"https:\/\/nilgamsafar.com\/blog\/%D8%B3%DB%8C%D8%B3%D8%AA%D9%85-%D8%A7%D9%85%D8%AA%DB%8C%D8%A7%D8%B2%D8%AF%D9%87%DB%8C-%D8%B3%D8%A7%D8%B2%D9%85%D8%A7%D9%86-%D9%85%D9%87%D8%A7%D8%AC%D8%B1%D8%AA-%D8%A7%D8%B3%D8%AA%D8%B1%D8%A7%D9%84\/\">\u0633\u06cc\u0633\u062a\u0645 \u0627\u0645\u062a\u06cc\u0627\u0632\u0628\u0646\u062f\u06cc \u0645\u0647\u0627\u062c\u0631\u062a \u0628\u0647 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627<\/a>\u00a0\u0631\u0627 \u0627\u06cc\u062c\u0627\u062f \u06a9\u0631\u062f\u0647 \u0627\u0633\u062a. \u0627\u06cc\u0646 \u0633\u06cc\u0633\u062a\u0645 \u0628\u0631 \u0627\u0633\u0627\u0633 \u0645\u0639\u06cc\u0627\u0631\u0647\u0627\u06cc \u0645\u062e\u062a\u0644\u0641 \u0627\u0642\u062f\u0627\u0645 \u0628\u0647 \u0645\u062d\u0627\u0633\u0628\u0647 \u0627\u0645\u062a\u06cc\u0627\u0632\u0627\u062a \u0645\u062a\u0642\u0627\u0636\u06cc\u0627\u0646 \u0645\u06cc\u200c\u06a9\u0646\u062f. \u0628\u0631\u0627\u06cc \u0627\u06cc\u0646 \u06a9\u0647 \u0648\u0627\u062c\u062f \u0634\u0631\u0627\u06cc\u0637 \u0645\u0647\u0627\u062c\u0631\u062a \u0628\u0647 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0628\u0627\u0634\u06cc\u062f \u0628\u0627\u06cc\u062f \u0628\u062a\u0648\u0627\u0646\u06cc\u062f \u062d\u062f \u0646\u0635\u0627\u0628 \u0645\u0648\u0631\u062f \u0646\u06cc\u0627\u0632 \u0627\u06cc\u0646 \u0633\u06cc\u0633\u062a\u0645 \u0631\u0627 \u06a9\u0633\u0628 \u06a9\u0646\u06cc\u062f. \u0628\u0646\u0627\u0628\u0631\u0627\u06cc\u0646 \u0642\u0628\u0644 \u0627\u0632 \u062c\u0633\u062a\u062c\u0648 \u0628\u0631\u0627\u06cc \u06cc\u0627\u0641\u062a\u0646 \u0634\u063a\u0644 \u062f\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0648 \u0627\u0631\u0633\u0627\u0644 EOI \u0628\u0647\u062a\u0631 \u0627\u0633\u062a \u06a9\u0647 \u0634\u0631\u0627\u06cc\u0637 \u062e\u0648\u062f \u0631\u0627 \u0627\u0632 \u0637\u0631\u06cc\u0642\u00a0<a href=\"https:\/\/immi.homeaffairs.gov.au\/help-support\/tools\/points-calculator\">\u0645\u062d\u0627\u0633\u0628\u0647 \u0622\u0646\u0644\u0627\u06cc\u0646 \u0627\u0645\u062a\u06cc\u0627\u0632 \u0645\u0647\u0627\u062c\u0631\u062a \u0628\u0647 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627<\/a>\u00a0\u0645\u0648\u0631\u062f \u0628\u0631\u0631\u0633\u06cc \u0642\u0631\u0627\u0631 \u062f\u0647\u06cc\u062f.<\/p>\n<p>\u06f4- \u0627\u0641\u0632\u0627\u06cc\u0634 \u0634\u0627\u0646\u0633 \u062f\u0631\u06cc\u0627\u0641\u062a \u0648\u06cc\u0632\u0627\u06cc \u06a9\u0627\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627<\/p>\n<p>\u0628\u0631\u0627\u06cc \u0627\u06cc\u0646 \u06a9\u0647 \u0628\u062a\u0648\u0627\u0646\u06cc\u062f \u0627\u0645\u062a\u06cc\u0627\u0632\u0627\u062a \u0628\u0627\u0644\u0627\u062a\u0631\u06cc \u0631\u0627 \u062f\u0631 \u0633\u06cc\u0633\u062a\u0645 \u0627\u0645\u062a\u06cc\u0627\u0632\u0628\u0646\u062f\u06cc \u0645\u0647\u0627\u062c\u0631\u062a \u0628\u0647 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0628\u0647 \u062f\u0633\u062a \u0628\u06cc\u0627\u0648\u0631\u06cc\u062f \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u06cc\u062f \u0628\u0631\u062e\u06cc \u0627\u0642\u062f\u0627\u0645\u0627\u062a \u0631\u0627 \u0627\u0646\u062c\u0627\u0645 \u062f\u0647\u06cc\u062f. \u0627\u0632 \u062c\u0645\u0644\u0647 \u0627\u06cc\u0646 \u0627\u0642\u062f\u0627\u0645\u0627\u062a \u0645\u06cc\u200c\u062a\u0648\u0627\u0646 \u0628\u0647 \u0628\u0647\u0628\u0648\u062f \u0645\u0647\u0627\u0631\u062a \u0632\u0628\u0627\u0646 \u0627\u0646\u06af\u0644\u06cc\u0633\u06cc \u062c\u0647\u062a \u06a9\u0633\u0628 \u0646\u0645\u0631\u0647 \u0628\u0627\u0644\u0627 \u062f\u0631 \u0622\u0632\u0645\u0648\u0646 IELTS \u0627\u0634\u0627\u0631\u0647 \u062f\u0627\u0634\u062a. \u0647\u0645\u0686\u0646\u06cc\u0646 \u0628\u0627 \u06af\u0630\u0631\u0627\u0646\u062f\u0646 \u062f\u0648\u0631\u0647\u200c\u0647\u0627\u06cc \u062d\u0631\u0641\u0647\u200c\u0627\u06cc \u062f\u0631 \u062d\u0648\u0632\u0647 \u062a\u062e\u0635\u0635\u06cc \u062e\u0648\u062f\u062a\u0627\u0646 \u0646\u06cc\u0632 \u0634\u0627\u0646\u0633 \u062e\u0648\u062f \u0631\u0627 \u0628\u0631\u0627\u06cc \u0627\u062e\u0630 \u0648\u06cc\u0632\u0627\u06cc \u06a9\u0627\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u0628\u06cc\u0634\u062a\u0631 \u062e\u0648\u0627\u0647\u06cc\u062f \u06a9\u0631\u062f. \u06cc\u0627\u0641\u062a\u0646 \u0634\u063a\u0644 \u062f\u0631 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627 \u06a9\u0627\u0645\u0644\u0627 \u0628\u0633\u062a\u06af\u06cc \u0628\u0647 \u0646\u0648\u0639 \u0648 \u0633\u0637\u062d \u0645\u0647\u0627\u0631\u062a \u0634\u062e\u0635 \u0645\u062a\u0642\u0627\u0636\u06cc \u062f\u0627\u0631\u062f.<\/p>\n<p>\u062f\u0631 \u0635\u0648\u0631\u062a\u06cc \u06a9\u0647 \u062a\u062e\u0635\u0635 \u0648 \u0645\u0647\u0627\u0631\u062a \u0634\u0645\u0627 \u0645\u0648\u0631\u062f \u0646\u06cc\u0627